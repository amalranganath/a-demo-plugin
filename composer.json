{
    "name": "amalranganath/a-demo-plugin",
    "description": "A  Framework Demo Plugin",
    "keywords": [
        "a demo plugin",
        "a framework",
        "demo plugin"
    ],
    "homepage": "https://github.com/amalranganath/a-demo-plugin",
    "type": "project",
    "license": "GPLv2",
    "authors": [
        {
            "name": "Amal Ranganath",
            "homepage": "https://www.facebook.com/amalranganath"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "amalranganath/a": "*",
        "stripe/stripe-php": "*"
    },
    "autoload": {
        "classmap": ["vendor/amalranganath/a"]
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}